<html lang="en" class="no-js">
<div id="search_brand_dlg" class="modal fade" tabindex="-1">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-body">
				<table class="table" id="sbr_table">
					<tr>
						<td align="center" style="border-bottom: none" colspan="2"><strong class="label label-green">Search Brand</strong></td>
					</tr>
					<tr>
						<td id="sbr_manufacturer" align="center" style="font-weight: bold;" colspan="2"></td>
					</tr>
					<tr>
						<td><input type="text" id="sbr_name" class="form-control" placeholder="Brand Name"></td>
						<td width="30"><button type="button" class="btn default" id="sbrbyname_btn">Search</button></td>
					</tr>
				</table>
				<table class="table" id="sbr_dialog_table_body">
				</table>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn yellow aboutme" id="add_new_brand_btn">Add New</button>
				<button type="button" class="btn yellow aboutme" id="search_existing_brand_btn" style="display: none;">Search Existing</button>
				<button type="button" data-dismiss="modal" class="btn default" id="sbr_dlg_btn">Cancel</button>
			</div>
		</div>
	</div>
</div>
</html>